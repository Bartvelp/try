mport requests
from bs4 import BeautifulSoup


url = 'https://www.bing.com/search?q=ethereum+koers&qs=n&form=QBRE&sp=-1&pq=ethereum+koers&sc=8-14&sk=&cvid=9B71BEBAC7F34A71BB73F922E5AFE304'
req = requests.get(url)
scrap = BeautifulSoup(req.text, 'html.parser')

eth_price = scrap.find('div', class_="b_focusTextSmall curr_totxt").text
print(eth_price)
